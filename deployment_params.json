{
  "github_url": "https://github.com/Traia-IO/test-serper-api-mcp-server",
  "mcp_server": {
    "name": "test-serper-api-mcp",
    "description": "Serper internet data api",
    "server_type": "streamable-http",
    "requires_api_key": true,
    "api_key_header": "Authorization",
    "api_keys": [
      "SERPER_API_KEY"
    ],
    "capabilities": [
      "serper_search",
      "serper_news",
      "serper_scholar"
    ],
    "payment_protocol": "402",
    "dual_mode": true,
    "dual_mode_description": "Supports both authenticated (free with API key) and payment (402 protocol) modes"
  },
  "deployment_method": "cloud_run",
  "gcp_project_id": "traia-mcp-servers",
  "gcp_region": "us-central1",
  "tags": [
    "test serper api",
    "api",
    "mcp"
  ],
  "ref": "main"
}